        <div class="row">
            <div class="col-md-12" ng-class="{'has-error' : branchform.businessName.$invalid}">
              
                <label class="control-label" for="businessName">Business / Legal Name</label>
                @Html.EditorFor(model => model.legalName, new { htmlAttributes = new { @class = "form-control" } })
                @Html.ValidationMessageFor(model => model.legalName, "", new { @class = "text-danger" })
            </div>
        </div>
        <div class="row">
            <div class="col-md-12" ng-class="{'has-error' : branchform.branch.$invalid}">
                <label class="control-label" for="branch">Branch Name</label>
                <input class="form-control" type="text" id="branchbranch" name="branch" required ng-model="$ctrl.Branch.branch" />
                <input-error error="branchform.branch.$error"></input-error>
            </div>
        </div>
        <div class="row">
            <div class="col-md-4" ng-class="{'has-error' : branchform.DBAName.$invalid}">
                <label class="control-label" for="DBAName">DBA (Branch / Trade Name)</label>
                @Html.EditorFor(model => model.DBAName, new { htmlAttributes = new { @class = "form-control" } })
                @Html.ValidationMessageFor(model => model.DBAName, "", new { @class = "text-danger" })
            </div>
            <div class="col-md-4" ng-class="{'has-error' : branchform.businessRegNo.$invalid}">
                <label class="control-label" for="businessRegNo">Registered Business Number</label>
                <input class="form-control" type="text" id="businessRegNo" name="businessRegNo" disabled ng-model="$ctrl.Branch.businessRegNo" />
                <input-error error="branchform.businessRegNo.$error"></input-error>
            </div>
            <div class="col-md-4">
                <label class="control-label" for="adminContactperson">Administrator (Contact Person)</label>
                @Html.EditorFor(model => model.adminContactperson, new { htmlAttributes = new { @class = "form-control" } })
                @Html.ValidationMessageFor(model => model.adminContactperson, "", new { @class = "text-danger" })
            </div>
        </div>
        <div class="row">
            <div class="col-md-12" ng-class="{'has-error' : branchform.dbaAddress1.$invalid}">
                <label class="control-label" for="goodsServices">DBA (Branch / Outlet Address)</label>
                @Html.EditorFor(model => model.dbaAddress1, new { htmlAttributes = new { @class = "form-control" } })
                @Html.ValidationMessageFor(model => model.dbaAddress1, "", new { @class = "text-danger" })
                @Html.EditorFor(model => model.dbaAddress2, new { htmlAttributes = new { @class = "form-control" } })
                @Html.ValidationMessageFor(model => model.dbaAddress2, "", new { @class = "text-danger" })
                @Html.EditorFor(model => model.dbaAddress3, new { htmlAttributes = new { @class = "form-control" } })
                @Html.ValidationMessageFor(model => model.dbaAddress3, "", new { @class = "text-danger" })
                @Html.EditorFor(model => model.dbaAddress4, new { htmlAttributes = new { @class = "form-control" } })
                @Html.ValidationMessageFor(model => model.dbaAddress4, "", new { @class = "text-danger" })
            </div>
        </div>
        <div class="row">
            <div class="col-md-6" ng-class="{'has-error' : branchform.dbaCity.$invalid}">
                <label class="control-label" for="dbaCity">DBA City</label>
                <select name="" class="form-control" ng-model="$ctrl.Branch.dbaCity" ng-options="city.City_Id as city.Description for city in $ctrl.cityList">
                    <option value="0" selected>Select City</option>
                </select>
                <input-error error="branchform.dbaCity.$error"></input-error>
            </div>
            <div class="col-md-6" ng-class="{'has-error' : branchform.dbaZipCode.$invalid}">
                <label class="control-label" for="dbaZipCode">ZipCode</label>
                <select name="" class="form-control" ng-model="$ctrl.Branch.dbaZipCode" ng-options="zipCode.ZipCode_Id as zipCode.Description for zipCode in $ctrl.zipCodeList">
                    <option value="0" selected>Select Zipcode</option>
                </select>
                <input-error error="branchform.dbaZipCode.$error"></input-error>
            </div>
        </div>
        <div class="row">
            <div class="col-md-12" ng-class="{'has-error' : branchform.goodsServices.$invalid}">
                <label class="control-label" for="goodsServices">Description of Goods, Products and Services</label>
                <input class="form-control" type="text" id="goodsServices" name="goodsServices" ng-model="$ctrl.Branch.goodsServices" />
                <input-error error="branchform.goodsServices.$error"></input-error>
            </div>
        </div>
        <div class="row">
            <div class="col-md-3" ng-class="{'has-error' : branchform.phoneNo.$invalid}">
                <label class="control-label" for="phoneNo">Branch / Outlet Phone Number</label>
                <input class="form-control" type="text" id="phoneNo" name="phoneNo" ng-model="$ctrl.Branch.phoneNo" />
                <input-error error="branchform.phoneNo.$error"></input-error>
            </div>
            <div class="col-md-3" ng-class="{'has-error' : branchform.mobileNo.$invalid}">
                <label class="control-label" for="mobileNo">Mobile Number</label>
                <input class="form-control" type="text" id="mobileNo" name="mobileNo" ng-model="$ctrl.Branch.mobileNo" />
                <input-error error="branchform.mobileNo.$error"></input-error>
            </div>
            <div class="col-md-3" ng-class="{'has-error' : branchform.emailAddress.$invalid}">
                <label class="control-label" for="emailAddress">E-Mail Address</label>
                <input class="form-control" type="email" id="emailAddress" required name="emailAddress" ng-model="$ctrl.Branch.emailAddress" />
                <input-error error="branchform.emailAddress.$error"></input-error>
            </div>
            <div class="col-md-3" ng-class="{'has-error' : branchform.website.$invalid}">
                <label class="control-label" for="website">Website</label>
                <input class="form-control" type="text" id="website" name="website" ng-model="$ctrl.Branch.website" />
                <input-error error="branchform.website.$error"></input-error>
            </div>
        </div>
        <div class="row">
            <div class="col-md-2" ng-class="{'has-error' : branchform.payment.$invalid}">
                <div class="col-md-12 row">
                    <label class="control-label" for="paymentMode">Payment Method (Please Select)</label>
                </div>
                <div class="col-md-6 row">
                    <label><input type="radio" name="paymentMode" value="check" ng-model="$ctrl.Branch.paymentMode" /> Check</label>
                </div>
                <div class="col-md-6 row">
                    <label><input type="radio" name="paymentMode" value="autoCredit" ng-model="$ctrl.Branch.paymentMode" /> Auto Credit</label>
                </div>
                <input-error error="branchform.payment.$error"></input-error>
            </div>
            <div class="col-md-2" ng-class="{'has-error' : branchform.settlementAcctNo.$invalid}">
                <label class="control-label" for="settlementAcctNo">Settlement Account Number (For Crediting Payment)</label>
                <input class="form-control" type="text" id="settlementAcctNo" name="settlementAcctNo" ng-model="$ctrl.Branch.settlementAcctNo" />
                <input-error error="branchform.settlementAcctNo.$error"></input-error>
            </div>
            <div class="col-md-2">
                <div class="col-md-12 row" ng-class="{'has-error' : branchform.withTax.$invalid}">
                    <label class="control-label" for="withTax">Tax Code (Please Select)</label>
                </div>
                <div class="col-md-12 row" ng-class="{'has-error' : branchform.withTax.$invalid}">
                    <label><input type="checkbox" id="withTax" name="withTax" ng-model="$ctrl.Branch.withTax" /> With Tax</label>
                </div>
                <input-error error="branchform.withTax.$error"></input-error>
            </div>
            <div class="col-md-3" ng-class="{'has-error' : branchform.taxExemptFrom.$invalid || branchform.taxExemptTo.$invalid}">
                <label class="control-label" for="taxExemptValidity">Tax Exempt Validity (mm/dd/yyyy)</label>
                <div class="input-group">
                    <input class="form-control datefield" type="text" placeholder="DATE FROM" id="" name="taxExemptFrom" ng-model="$ctrl.Branch.taxExemptFrom" />

                </div>
                <input-error error="branchform.taxExemptFrom.$error"></input-error>
                <div class="input-group">
                    <input class="form-control" type="text" uib-datepicker-popup="{{$ctrl.format}}" datepicker-options="$ctrl.dateOptions" placeholder="DATE TO" id="" is-open="$ctrl.datepickerObj.taxExemptTo" name="taxExemptTo" ng-model="$ctrl.Branch.taxExemptTo" />
                    <span class="input-group-btn">
                        <button type="button" class="btn btn-default btn-sm" ng-click="$ctrl.toggleDatepicker('taxExemptTo')"><i class="fa fa-calendar"></i></button>
                    </span>
                </div>
                <input-error error="branchform.taxExemptTo.$error"></input-error>
            </div>
            <div class="col-md-3" ng-class="{'has-error' : branchform.taxExemptIssuedBy.$invalid}">
                <label class="control-label" for="taxExemptIssuedBy">Tax Exempt Certificate Issued By</label>
                <select class="form-control" ng-model="$ctrl.Branch.taxExemptIssuedBy" ng-options="tax.TaxExemptionCertificateIssuedBy_Id as tax.Description for tax in $ctrl.textExemptIssuedByList"></select>
                <input-error error="branchform.taxExemptIssuedBy.$error"></input-error>
            </div>
        </div>
        <div class="row">
            <div class="col-md-4" ng-class="{'has-error' : branchform.TIN.$invalid}">
                <label class="control-label" for="TIN">Tax Identification Number (TIN)</label>
                <input class="form-control" type="text" id="TIN" name="TIN" ng-model="$ctrl.Branch.TIN" />
                <input-error error="branchform.TIN.$error"></input-error>
            </div>
            <div class="col-md-8">
                <div class="col-md-12 row" ng-class="{'has-error' : branchform.taxType.$invalid}">
                    <label class="control-label" for="taxType">Tax Type (Please Select)</label>
                </div>
                <div class="col-md-6 row">
                    <label class="control-label" for="sp"><input type="checkbox" name="sp" ng-model="$ctrl.Branch.isSingleProp" /> Single Proprietorship (WI 156)</label>
                    <div class="col-md-12" ng-class="{'has-error' : branchform.DTIBusinessRegDate.$invalid}">
                        <label class="control-label" for="DTIBusinessRegDate">DTI Business Registration Date (mm/dd/yyyy)</label>
                        <div class="input-group">
                            <input class="form-control" type="text" uib-datepicker-popup="{{$ctrl.format}}" datepicker-options="$ctrl.dateOptions" id="DTIBusinessRegDate" is-open="$ctrl.datepickerObj.DTIBusinessRegDate" name="DTIBusinessRegDate" ng-model="$ctrl.Branch.DTIBusinessRegDate" />
                            <span class="input-group-btn">
                                <button type="button" class="btn btn-default btn-sm" ng-click="$ctrl.toggleDatepicker('DTIBusinessRegDate')"><i class="fa fa-calendar"></i></button>
                            </span>
                        </div>
                        <input-error error="branchform.DTIBusinessRegDate.$error"></input-error>
                    </div>
                </div>
                <div class="col-md-6 row">
                    <label class="control-label" for="pc"><input type="checkbox" name="pc" ng-model="$ctrl.Branch.isPartnershipCorp" /> Partnership / Corporation</label>
                    <div class="col-md-12" ng-class="{'has-error' : branchform.SECRegDate.$invalid}">
                        <label class="control-label" for="SECRegDate">SEC Registration Date (mm/dd/yyyy)</label>
                        <div class="input-group">
                            <input class="form-control" type="text" uib-datepicker-popup="{{$ctrl.format}}" datepicker-options="$ctrl.dateOptions" id="SECRegDate" is-open="$ctrl.datepickerObj.SECRegDate" name="SECRegDate" ng-model="$ctrl.Branch.SECRegDate" />
                            <span class="input-group-btn">
                                <button type="button" class="btn btn-default btn-sm" ng-click="$ctrl.toggleDatepicker('SECRegDate')"><i class="fa fa-calendar"></i></button>
                            </span>
                        </div>
                        <input-error error="branchform.SECRegDate.$error"></input-error>
                    </div>
                </div>
            </div>
        </div>
        <div class="row">
            <div class="col-md-12" ng-class="{'has-error' : branchform.principalOwner.$invalid}">
                <label class="control-label" for="principalOwner">Name of Principal Owner</label>
                <input class="form-control" type="text" id="principalOwner" name="principalOwner" ng-model="$ctrl.Branch.principalName1" />
                <input-error error="branchform.principalOwner.$error"></input-error>
            </div>
            <div class="col-md-12" ng-class="{'has-error' : branchform.companySignatory.$invalid}">
                <label class="control-label" for="companySignatory">Name of Company Signatory/ies</label>
                <input class="form-control" type="text" id="principalName2" name="principalName2" ng-model="$ctrl.Branch.principalName2" />
                <input class="form-control" type="text" id="companySignatory" name="principalName3" ng-model="$ctrl.Branch.principalName3" />
                <input class="form-control" type="text" id="principalName4" name="principalName4" ng-model="$ctrl.Branch.principalName4" />
                <input class="form-control" type="text" id="principalName5" name="principalName5" ng-model="$ctrl.Branch.principalName5" />
                <input-error error="branchform.companySignatory.$error"></input-error>
            </div>
        </div>
        <div class="row">
            <div class="col-md-12" ng-class="{'has-error' : branchform.mailingAddress1.$invalid}">
                <label class="control-label" for="mailingAddress">Special / Mailing Address - For Centralized Delivery of CWT (Department, Floor &amp; Building Name, Street, District / City, Province, Country, Zip Code)</label>
                <input class="form-control" type="text" id="mailingAddress1" name="mailingAddress1" ng-model="$ctrl.Branch.mailingAddress1" />
                <input class="form-control" type="text" id="mailingAddress2" name="mailingAddress2" ng-model="$ctrl.Branch.mailingAddress2" />
                <input class="form-control" type="text" id="mailingAddress3" name="mailingAddress3" ng-model="$ctrl.Branch.mailingAddress3" />
                <input class="form-control" type="text" id="mailingAddress4" name="mailingAddress4" ng-model="$ctrl.Branch.mailingAddress4" />
                <input-error error="branchform.mailingAddress1.$error"></input-error>
            </div>
        </div>
        <div class="row">
            <div class="col-md-6" ng-class="{'has-error' : branchform.mailingCity.$invalid}">
                <label class="control-label">City</label>
                <select class="form-control" name="mailingCity" ng-model="$ctrl.Branch.mailingCity" ng-options="city.City_Id as city.Description for city in $ctrl.cityList">
                    <option value="0" selected>Select City</option>
                </select>
                <input-error error="branchform.mailingCity.$error"></input-error>
            </div>
            <div class="col-md-6" ng-class="{'has-error' : branchform.mailingAddressZipCode.$invalid}">
                <label class="control-label">ZipCode</label>
                <select class="form-control" name="mailingAddressZipCode" ng-model="$ctrl.Branch.mailingAddressZipCode" ng-options="zipCode.ZipCode_Id as zipCode.Description for zipCode in $ctrl.zipCodeList">
                    <option value="0" selected>Select Zipcode</option>
                </select>
                <input-error error="branchform.mailingAddressZipCode.$error"></input-error>
            </div>
        </div>
        <div class="row">
            <div class="col-md-4" ng-class="{'has-error' : branchform.withAcquirer.$invalid}">
                <label><input type="checkbox" name="withAcquirer" ng-model="$ctrl.Branch.withAcquirer" /> With Existing Acquirer?</label>
                <input-error error="branchform.withAcquirer.$error"></input-error>
            </div>
            <div class="col-md-8" ng-class="{'has-error' : branchform.acquirerInstitutionName.$invalid}">
                <label class="control-label" for="acquirerInstitutionName">Name of Institution</label>
                <input class="form-control" type="text" id="acquirerInstitutionName" name="acquirerInstitutionName" ng-model="$ctrl.Branch.acquirerInstitutionName" />
                <input-error error="branchform.acquirerInstitutionName.$error"></input-error>
            </div>
        </div>

        <div class="row clearfix-custom">
            <div class="form-header clearfix-custom">
                <p class="header-title">
                    Storage of Card Holder Information
                </p>
            </div>
        </div>
        <div class="row">
            <div class="col-md-6" ng-class="{'has-error' : branchform.withECR.$invalid}">
                <label class="control-label" for="withECR"><input type="checkbox" name="withECR" ng-model="$ctrl.Branch.withECR" /> With Installed Electronic Cash Register (ECR)</label>
                <input-error error="branchform.withECR.$error"></input-error>
            </div>
            <div class="col-md-6">
                <div class="col-md-12 row" ng-class="{'has-error' : branchform.serviceProvider.$invalid}">
                    <label class="control-label col-md-3" for="serviceProvider">Service Provider</label>
                    <div class="col-md-9 row">
                        <input class="form-control" type="text" id="serviceProvider" name="serviceProvider" ng-disabled="$ctrl.Branch.withEcr" ng-model="$ctrl.Branch.serviceProvider" />
                        <small class="text-muted">IF WITH INSTALLED ECR</small>
                        <input-error error="branchform.serviceProvider.$error"></input-error>
                    </div>
                </div>
                <div class="col-md-12 row" ng-class="{'has-error' : branchform.ecrForInstallation.$invalid}">
                    <label class="control-label" for="ecrForInstallation"><input type="checkbox" id="ecrForInstallation" ng-if="!$ctrl.Branch.withEcr" name="ecrForInstallation" ng-model="$ctrl.Branch.ecrForInstallation" /> For Installation</label>
                    <div class="col-md-12 row">
                        <small class="text-muted">IF WITHOUT INSTALLED ECR</small>
                    </div>
                    <input-error error="branchform.ecrForInstallation.$error"></input-error>
                </div>
            </div>
        </div>
