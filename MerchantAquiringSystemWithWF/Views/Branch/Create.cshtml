                <input class="form-control" type="text" id="debitSettlementAcctNo" name="debitSettlementAcctNo" ng-model="$ctrl.Branch.debitSettlementAcctNo" />
                <input-error error="branchform.debitSettlementAcctNo.$error"></input-error>
